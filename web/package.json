    "runtimeCaching": [
      {
        "urlPattern": [
          "/$"
        ],
        "handler": "networkFirst"
      },
      {
        "urlPattern": [
          "/.+"
        ],
        "handler": "networkFirst",
        "options": {
          "cacheName": "workbox-temporary",
          "expiration": {
            "maxAgeSeconds": 86400,
            "maxEntries": 10
          }
        }
      }
